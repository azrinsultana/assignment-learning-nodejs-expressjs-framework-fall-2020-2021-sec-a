<div class="widget search-widget">
  <h5>Search</h5>
  <form class="search-form">
      <div class="input-group">
          <input type="text" placeholder="Search..." id="search" class="form-control">
          <div class="input-group-append">
              <button type="submit" class="btn btn-primary"><i class="fa fa-search"></i></button>
          </div>
      </div>
  </form>
</div>

<div class="container">
  
  <div class="row">
    <% 
  medicine.forEach( function(std){ %>
 
    <div class="col-md-4">
    
      <div class="card" style="width: 18rem;">
  <img class="card-img-top" src="..." alt="Card image cap">
  <div class="card-body">
    
    <h5 class="card-title"><%= std.medicinename %></h5>
    <label >vandor Name :<%= std.vendorname %></label><br>
    <label >Price =<%= std.price %> tk </label><br>
    <label > Quantity =<%= std.quantity%></label><br>

<a href="/admin/medicineview/<%= std.id %>" class="btn btn-primary btn-rounded ">View</a>
  </div>
</div>

  
    </div>
    <%
  }); 
  %>
  </div>
</div>

<div class="sidebar-overlay" data-reff=""></div>
<script src="/abc/js/jquery-3.2.1.min.js"></script>
