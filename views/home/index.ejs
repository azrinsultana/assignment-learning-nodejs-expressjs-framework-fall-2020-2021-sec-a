<script src="/abc/js/app.js"></script>
<script>
    $(document).ready(function(){
        $("#search").keyup(function(){
    var keyword = $(this).val();
     
      if (keyword != "") {
        $.ajax({
          url: '/admin/search',
          method: 'POST',
         
          data: {keyword:keyword},
          success: function(results){
            $("#output").empty(); 
            results.forEach(myFunction);

function myFunction(blog) {

            document.getElementById("output").innerHTML +=' <a href="/admin/medicineview/'+blog.id+'   ">' +blog.medicinename +'</a>' +" <br>"; 
            
        }
 
            

            $("#search").focusin(function(){
                $('#output').css('display', 'block');
            });
          }
        });
      } else {
     
    }
  });

    });
</script>
