<table border="1" class="table">
	<tr>
		
		<td class="bold">Medicine Name</td>
		
		<td class="bold">Genre</td>
		<td class="bold">Medicine Type</td>
		<td class="bold">Vendor Name</td>
		<td class="bold">Price</td>
		<td class="bold">Quantity</td>
		<td class="bold">Action</td>

		
	</tr>
	<td>
		
		<% 
			medicine.forEach( function(std){ %>
			<tr>
				
				<td><%= std.medicinename %></td>
				<td><%= std.genre%></td>
				<td><%= std.medicinetype %></td>
				<td><%= std.vendorname %></td>
				<td><%= std.price %> </td>
				<td><%= std.quantity%> </td>

				<td>
					<a href="/admin/medicineedit/<%= std.id %>">Edit</a> |
					<a href="/admin/medicinedelete/<%= std.id %>">Delete</a>
				</td>
			</tr>
		<%
			}); 
		%>
</table>


